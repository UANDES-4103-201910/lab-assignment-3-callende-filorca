<style media="screen">
  input[type=text], select {
  width: 100%;
  padding: 12px 30px;
  margin: 8px 0;
  display: inline-block;
  border: 1px solid #ccc;
  border-radius: 4px;
  box-sizing: border-box;
  }

  textarea {
    width: 100%;
    padding: 12px 20px;
    margin: 8px 0;
    display: inline-block;
    border: 1px solid #ccc;
    border-radius: 4px;
    box-sizing: border-box;
  }

  input[type=submit] {
  width: 100%;
  margin-top: 0%;
  width: fit-content ;
  position: relative;
  border-color: #ff4900!important;
  background-color: #ffd9b3!important;
  color: #ff4900!important;
  }



</style>



<%= form_with(model: post, local: true) do |form| %>
  <% if post.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(post.errors.count, "error") %> prohibited this post from being saved:</h2>

      <ul>
      <% post.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>
<!--DESDE AQUI -->
<div class="container">
  <div class="row">

    <div class="col-md-6 col-md-offset-0">

      <!-- Checkbox -->
      <div class="[ form-group ]">
        <input type="checkbox" name="fancy-checkbox-warning" id="fancy-checkbox-warning" autocomplete="off" />
        <div class="[ btn-group ]">
            <label for="fancy-checkbox-warning" class="[ btn btn-warning ]">
                <span class="[ glyphicon glyphicon-ok ]"></span>
                <span> </span>
            </label>
            <label for="fancy-checkbox-warning" class="[ btn btn-default active ]">
                Closed Post
            </label>
        </div>
      </div>

          <h4> Title </h4>
          <%= form.text_field :title %>

          <h4> Location </h4>
          <%= form.text_field :location %>

          <h4> Country </h4>
          <%= form.text_field :country %>

          <h4> City </h4>
          <%= form.text_field :city %>

          <h4> Description </h4>
          <%= form.text_area :description, rows:"5", :placeholder  => "Description" %>

          <br><br>
          <div class="field">

                <%= form.hidden_field :user_id, value: current_user.id %>
              </div>



              <div class="actions">
                <%= form.submit "Submit", class: "btn btn-primary"%>
                <a  class="btn btn-primary" href="/posts/<%= @post.id %>">Cancel</a>
              </div>







    </div>




<div class="col-md-4 col-md-offset-2">

  <h1>Add File</h1>


      <div class="form-group">
        <div class ="form_group">
          <div class="input-group">
            <%= form.file_field :image, direct_upload: true %>
          </div>
        </div>
      </div>


      <div class="form-group">
          <figure class="img-responsive">
            <img src="data:image/png;base64," width="50%" height="50%">

          </figure>
      </div>

      <div class="form-group">
          <figure class="img-responsive">
            <img src="data:image/png;base64," width="50%" height="50%">

          </figure>
      </div>

      <div class="form-group">
          <figure class="img-responsive">
            <img src="data:image/png;base64," width="50%" height="50%">

          </figure>
      </div>

</div>
</div>
</div>

<% end %>
